#include <iostream>
using namespace std ;
int main() {
    string Alphabet1,Alphabet2,Alphabet3;
   int i = 1;
while(i<= 3){

cout<<" Please enter an alphabet:  \t  " ;
cin>>Alphabet1;
i++;
if (Alphabet1== "a" || Alphabet1== "e" || Alphabet1== "i" || Alphabet1== "o" || Alphabet1== "u" ) {
    
    cout << " *| Your first alphabet is a vowel |* \n " ;
}
 else {
     cout << " *| Your Alphabet is a consonant |* \n " ;
 }   
    
}   
    
}
